<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bluo.service.user.mapper.LikesMapper">

    <select id="getBlogIdByUserId" resultType="java.lang.Integer">
        select blog_id
        from likes
        where user_id = #{userId}
    </select>

</mapper>